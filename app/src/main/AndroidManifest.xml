<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhanghua.databindingstudy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sample.basic.BasicActivity"
            android:label="BasicActivity" />
        <activity
            android:name=".sample.custombinding.CustomBindingActivity"
            android:label="CustomBindingActivity" />
        <activity
            android:name=".sample.include.IncludeActivity"
            android:label="IncludeActivity" />
        <activity
            android:name=".sample.collection.CollectionActivity"
            android:label="CollectionActivity" />
        <activity
            android:name=".sample.resource.ResourceActivity"
            android:label="ResourceActivity" />
        <activity
            android:name=".sample.observable.ObservableActivity"
            android:label="ObservableActivity" />
        <activity
            android:name=".sample.viewWithIds.ViewWithIDsActivity"
            android:label="ViewWithIDsActivity" />
        <activity
            android:name=".sample.viewstub.ViewStubActivity"
            android:label="ViewStubActivity" />
        <activity
            android:name=".sample.dynamic.DynamicActivity"
            android:label="DynamicActivity" />
        <activity
            android:name=".sample.attributesetter.AttributeSettersActivity"
            android:label="AttributeSettersActivity" />
        <activity android:name=".sample.converter.ConversionsActivity"
            android:label="ConversionsActivity"/>
    </application>

</manifest>